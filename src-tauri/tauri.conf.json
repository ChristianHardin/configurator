{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "configurator",
  "version": "0.0.2",
  "identifier": "com.configurator.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Configurator",
        "width": 1280,
        "height": 720,
		"minWidth": 1280,
		"minHeight": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
"plugins": {
    "sql": {
      "preload": ["sqlite:main.db"]
    }
  }
}
